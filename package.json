    "clsx": "1.2.1",
    "esbuild": "0.14.51",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.3.0",
    "eslint-plugin-import": "2.26.0",
