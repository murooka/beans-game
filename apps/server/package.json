{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --ext ts --exec ts-node src/index.ts",
    "checkup": "prettier --check ./src && eslint ./src && tsc --noEmit && vitest run",
    "fix": "npm run format && npm run lint",
    "lint": "eslint --fix ./src",
    "format": "prettier -w ./src",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "express": "4.18.1",
    "socket.io": "4.5.1"
  },
  "devDependencies": {
    "socket.io-client": "4.5.1"
  }
}
